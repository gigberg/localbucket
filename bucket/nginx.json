{
    "version": "1.7.11.3",
    "description": "nginx with rtmp pre-compiled",
    "notes": "To use the correct configuration run 'nginx -p \"$env:NGINX_HOME\"' or 'nginx -p \"%NGINX_HOME%\"'",
    "url": "http://nginx-win.ecsds.eu/download/nginx%201.7.11.3%20Gryphon.zip",
    "bin": "nginx.exe",
    "env_set": {
        "NGINX_HOME": "$dir"
    },
    "persist": [
        "conf",
        "html",
        "logs",
        "temp"
    ]
}
